{% set version = {
    'bootstrap': '4.0.0-alpha',
    'jQuery'   : '2.1.4'
} %}<!DOCTYPE html>
<html lang="nl">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/{{ version.bootstrap }}/css/bootstrap.min.css">
        <title>{% block title %}NMDAD3 Demo{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
    {% include 'menu.html.twig' %}
        <div class="container">
            {% block body %}{% endblock %}
        </div>
        <script src="//code.jquery.com/jquery-{{ version.jQuery }}.min.js"></script>
        <script src="//maxcdn.bootstrapcdn.com/bootstrap/{{ version.bootstrap }}/js/bootstrap.min.js"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
